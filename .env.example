# Environment variables for ModelingEvolution.AutoUpdater
# Copy this file to .env and update with your values

# SSH Configuration for remote updates
SSH_USER=deploy
SSH_PASSWORD=your-secure-password

# Optional: Override default Docker host address
HOST_ADDRESS=172.17.0.1

# Docker Registry Authentication (base64 encoded username:password)
DOCKER_IO_AUTH=

# Git Configuration
GIT_USER_NAME=AutoUpdater
GIT_USER_EMAIL=updater@example.com

# Application Configuration
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://+:8080